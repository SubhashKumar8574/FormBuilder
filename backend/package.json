{
  "name": "form-builder-backend",
  "version": "1.0.0",
  "description": "Backend for the Form Builder application",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "server": "nodemon src/app.js",
    "client": "npm run dev --prefix ../frontend",
    "all": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "mongoose": "^5.13.23",
    "multer": "^1.4.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^2.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
